{% extends 'base.html.twig' %}

{% block title %}Stories - {{ parent() }}{% endblock %}

{% block body %}
    <h2 class="page-header">Stories</h2>
    <ul class="list-group">
        {% for story in stories %}
            <li class="list-group-item"><a href="{{ path('story_show', { storyId : story.id }) }}">{{ story.title }}</a></li>
        {% endfor %}
    </ul>
{% endblock %}
